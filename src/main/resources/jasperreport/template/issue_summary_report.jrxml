	<queryString language="SQL">
		<![CDATA[select project_uuid,
       project_id,
       project_name,
       project_owner_username,
       project_owner,
       file_info_tbl.scan_task_id,
       scan_time,
       scan_end_time,
       no_of_lines,
       no_of_file,
       rule_set_display_name,
       rule_set_version,
       rule_set_name,
       issue_total,
       level_1,
       level_2,
       level_3,
       level_1_critical,
       level_2_critical,
       level_3_critical,
       critical,
       level_1_definite,
       level_2_definite,
       level_3_definite,
       definite,
       maybe,
       confirmed,
       waived,
       pending,
       false_positive
from (select p.id project_uuid,
             p.project_id project_id,
             p.name project_name,
             p.created_by project_owner_username,
             u.display_name project_owner,
             st.id scan_task_id,
             st.scan_start_at scan_time,
             st.scan_end_at scan_end_time,
             sum(fi.no_of_lines) no_of_lines,
             count(1) no_of_file
      from scan_task st
               left join scan_file sf on (sf.scan_task_id = st.id)
               left join file_info fi on (fi.id = sf.file_info_id)
               left join project p on (p.id = st.project_id)
               left join "user" u on (u.username = p.created_by)
      where st.id::text = $P{SCAN_ID}
      group by p.id, st.id, u.display_name) as file_info_tbl
         left join
     (select st.id scan_task_id,
             COUNT(DISTINCT i.id) issue_total,
             SUM(CASE WHEN ri.priority = '1' THEN 1 ELSE 0 END) level_1,
             SUM(CASE WHEN ri.priority = '2' THEN 1 ELSE 0 END) level_2,
             SUM(CASE WHEN ri.priority = '3' THEN 1 ELSE 0 END) level_3,
             SUM(CASE WHEN ri.priority = '1' AND ri.certainty = 'CRITICAL' THEN 1 ELSE 0 END) level_1_critical,
             SUM(CASE WHEN ri.priority = '2' AND i.action = 'CRITICAL' THEN 1 ELSE 0 END) level_2_critical,
             SUM(CASE WHEN ri.priority = '3' AND i.action = 'CRITICAL' THEN 1 ELSE 0 END) level_3_critical,
             SUM(CASE WHEN ri.priority = '1' AND ri.certainty = 'D' THEN 1 ELSE 0 END) level_1_definite,
             SUM(CASE WHEN ri.priority = '2' AND ri.certainty = 'D' THEN 1 ELSE 0 END) level_2_definite,
             SUM(CASE WHEN ri.priority = '3' AND ri.certainty = 'D' THEN 1 ELSE 0 END) level_3_definite,
             SUM(CASE WHEN i.severity = 'CRITICAL' THEN 1 WHEN i.action = 'CRITICAL' THEN 1 ELSE 0 END) critical,
             SUM(CASE WHEN ri.certainty = 'D' THEN 1 ELSE 0 END) definite,
             SUM(CASE WHEN ri.certainty = 'M' THEN 1 ELSE 0 END) maybe,
             SUM(CASE WHEN i.action = 'CONFIRMED' THEN 1 ELSE 0 END) confirmed,
             SUM(CASE WHEN i.action = 'WAIVED' THEN 1 ELSE 0 END) waived,
             SUM(CASE WHEN i.action = 'FALSE_POSITIVE' THEN 1 ELSE 0 END) false_positive,
             SUM(CASE WHEN i.action = 'PENDING' THEN 1 ELSE 0 END) pending,
             rs.display_name rule_set_display_name,
             rs.name rule_set_name,
             rs.version rule_set_version
      from issue_v2 i
               left join scan_task st on (st.id = i.scan_task_id)
               left join rule_information ri on (ri.id = i.rule_information_id)
               left join rule_set rs on (rs.id = ri.rule_set_id)
      where i.scan_task_id::text = $P{SCAN_ID}
      and rs.id::text = $P{RULE_SET_ID}
      group by st.id, rs.name, rs.display_name, rs.version) as defect_tbl
     on (file_info_tbl.scan_task_id = defect_tbl.scan_task_id);]]>
	</queryString>
